# enable colored output from ls, etc
export CLICOLOR=1
export LSCOLORS="Exfxcxdxbxegedabagacad"

# load
# export PATH="$HOME/.rbenv/shims:$HOME/.rbenv/bin:node_modules/.bin:/usr/local/bin:/usr/local/sbin:$PATH"
# export CDPATH=".:$HOME/dev/"

export EDITOR="vim"

# Homebrew M1
export PATH=/opt/homebrew/bin:$PATH
export PATH=/opt/homebrew/sbin:$PATH

# tfenv M1
export TFENV_ARCH=arm64

# GO
export PATH="$(go env GOPATH)/bin:$PATH"

# GNU Binaries (i.e. sed)
# PATH="/usr/local/opt/gnu-sed/libexec/gnubin:$PATH"

# sess config
export SESS_PROJECT_ROOT="$HOME/Projects/work/:$HOME/Projects/personal/"

export FZF_DEFAULT_COMMAND='ag --path-to-ignore ~/.ag_ignore -l --nocolor --hidden -g ""'

# kubectx, kubens to path
export PATH="$HOME/bin:$PATH"
