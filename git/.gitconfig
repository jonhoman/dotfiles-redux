[color]
  ui = auto

[alias]
  st = status
  ci = commit
  co = checkout
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  cache = diff --cached
  amend = commit --amend --no-edit

[core]
  editor = /usr/bin/vim
[push]
  default = simple
[pager]
  branch = false

[commit]
  gpgsign = true
